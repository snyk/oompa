{
  "name": "snyk-oompa",
  "description": "A tiny pick-me-app for express-based, stateless, microservices.",
  "main": "server.js",
  "scripts": {
    "test": "snyk test && nyc ava -v",
    "html-report": "nyc ava --reporter=html && open coverage/index.html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Snyk/oompa.git"
  },
  "author": "Alon Niv",
  "keywords": [
    "microservices",
    "http",
    "stateless",
    "micro"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Snyk/oompa/issues"
  },
  "homepage": "https://github.com/Snyk/oompa#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "request": "^2.74.0",
    "snyk": "^1.30.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "coveralls": "^2.11.11",
    "nyc": "^6.6.1",
    "semantic-release": "^4.3.5",
    "then-sleep": "^1.0.1"
  },
  "snyk": true
}
